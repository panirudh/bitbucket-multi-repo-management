{
  "name": "@panirudh/bitbucket-multi-repo-management",
  "version": "2.0.0",
  "description": "A node.js console application that manages multiple git repositories in Bitbucket Cloud via the Bitbucket API.",
  "main": "bin/console.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/panirudh/bitbucket-multi-repo-management.git"
  },
  "author": "Praveen Anirudh <panirudh87@gmail.com>",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^20"
  },
  "bin": {
    "bmrm": "bin/console.js"
  },
  "bugs": {
    "url": "https://github.com/panirudh/bitbucket-multi-repo-management/issues"
  },
  "homepage": "https://github.com/panirudh/bitbucket-multi-repo-management#readme",
